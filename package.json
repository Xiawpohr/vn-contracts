{
  "name": "vn-contracts",
  "version": "1.0.0",
  "description": "contracts for value network",
  "scripts": {
    "node": "npx hardhat node",
    "console": "npx hardhat console",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "deploy": "npm run deploy:mainnet",
    "deploy:mainnet": "npx hardhat run --network mainnet scripts/deploy.js",
    "deploy:local": "npx hardhat run --network localhost scripts/deploy.js",
    "deploy:rinkeby": "npx hardhat run --network rinkeby scripts/deploy.js",
    "deploy:ropsten": "npx hardhat run --network ropsten scripts/deploy.js",
    "deploy:kovan": "npx hardhat run --network kovan scripts/deploy.js",
    "deploy:goerli": "npx hardhat run --network goerli scripts/deploy.js",
    "create-account": "npx mnemonics"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "defi"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts": "^3.3.0",
    "chai": "^4.2.0",
    "ethereum-waffle": "^3.2.1",
    "ethers": "^5.0.23",
    "hardhat": "^2.0.3"
  }
}
